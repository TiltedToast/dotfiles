[core]
    editor = code --wait
    sshCommand = /usr/local/bin/ssh
    excludesfile = ~/.gitignore
    pager = delta
[user]
    name = Tim
    email = 57221600+TiltedToast@users.noreply.github.com
    signingkey = ssh-rsa 
[filter "lfs"]
    clean = git-lfs clean -- %f
    smudge = git-lfs smudge -- %f
    process = git-lfs filter-process
    required = true
[commit]
    gpgsign = true
[gpg]
    format = ssh

[gpg "ssh"]
  program = "/usr/local/bin/op-ssh-sign-wsl"
[alias]
    wt = worktree
    logline = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
[credential]
    helper = "gh auth git-credential"
[init]
    defaultBranch = main
[rerere]
    enabled = true
[column]
    ui = never
[branch]
    sort = -committerdate
[maintenance]
    repo = ~/projects/hifumi
[merge]
    autoStash = true
    conflictstyle = diff3
[rebase]
    autoStash = true
[pull]
    rebase = true
[diff]
    colorMoved = default
[delta]
    navigate = true
[interactive]
    diffFilter = delta --color-only
